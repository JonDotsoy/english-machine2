---
import Layout from "../layouts/Layout.astro";
import MultiChoice from "../components/MultiChoice";
import phrasalVerbsMeanings from "../dbs/phrasal-verbs-meanings.json";
---

<Layout>
  <h1 class="text-2xl font-bold text-center my-6">Phrasal Verbs Meanings</h1>
  <div class="max-w-2xl mx-auto">
    <MultiChoice
      client:load
      type="phrasal-verbs-meanings"
      db={phrasalVerbsMeanings.questions}
    />
  </div>
</Layout>
